<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>

<SplitPane dividerPositions="0.2" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scribble.Controller">
   <items>
      <TextArea fx:id="textArea" focusTraversable="false" />
      <StackPane fx:id="root">
         <children>
            <Region fx:id="background" maxHeight="-Infinity" maxWidth="-Infinity" />
            <StackPane fx:id="animationPane" maxHeight="-Infinity" maxWidth="-Infinity" />
         </children>
      </StackPane>
   </items>
</SplitPane>
